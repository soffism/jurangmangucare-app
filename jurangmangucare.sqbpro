CREATE VIEW ViewRekapTransaksi AS
SELECT 
    KodeAnggota,
	NamaAnggota,
	JenisAnggota,
    KodeJenisTrans,
    JenisTransaksi,
    TotalJumlah
FROM (
    SELECT 
        KodeAnggota,
		NamaAnggota,
		JenisAnggota,
        KodeJenisTrans,
        JenisTransaksi,
        SUM(Jumlah) AS TotalJumlah
    FROM 
        ViewTransaksiLengkap
    GROUP BY 
        KodeAnggota,
		NamaAnggota,
        KodeJenisTrans,
        JenisTransaksi
)
WHERE TotalJumlah &lt;&gt; 0
ORDER BY TotalJumlah</sql><sql name="SQL 8">UPDATE Anggota
SET JenisAnggota = &quot;Orang JMCare&quot;
WHERE NoAnggota = 200;</sql><sql name="SQL 9*">DELETE FROM Anggota
