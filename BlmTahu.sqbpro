SET JenisAnggota = 'Orang MAS' 
WHERE JenisAnggota = 'orang MAS';
</sql><sql name="SQL 2*">DROP VIEW IF EXISTS ViewTransaksiLengkap;

CREATE VIEW ViewTransaksiLengkap AS
SELECT 
    T.No, 
    T.KdTanggal,
    H.Tanggal,
    A.NoAnggota, 
    T.KodeAnggota, 
    A.NamaAnggota, 
    T.KodeJenisTrans, 
    J.JenisTransaksi, 
    A.JenisAnggota,
    T.Uraian, 
    T.Jumlah
FROM 
    Transaksi T
    INNER JOIN Anggota A ON A.KodeAnggota = T.KodeAnggota
    INNER JOIN JenisTransaksi J ON J.KodeJenisTrans = T.KodeJenisTrans
    INNER JOIN HeaderTransaksi H ON H.IDTanggal = T.KdTanggal;
</sql><sql name="Edit View ViewRekapTransaksi*">DROP VIEW IF EXISTS &quot;main&quot;.&quot;ViewRekapTransaksi&quot;;
CREATE VIEW ViewRekapTransaksi AS
SELECT 
    KodeAnggota,
	NamaAnggota,
	JenisAnggota
    KodeJenisTrans,
    JenisTransaksi,
    TotalJumlah
FROM (
    SELECT 
        KodeAnggota,
		NamaAnggota,
		JenisAnggota
        KodeJenisTrans,
        JenisTransaksi,
        SUM(Jumlah) AS TotalJumlah
    FROM 
        ViewTransaksiLengkap
    GROUP BY 
        KodeAnggota,
		NamaAnggota,
        KodeJenisTrans,
        JenisTransaksi
)
WHERE TotalJumlah &lt;&gt; 0
ORDER BY TotalJumlah</sql><current_tab id="2"/></tab_sql></sqlb_project>
